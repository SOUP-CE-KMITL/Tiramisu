#!/usr/bin/env stap

global list

probe begin {
	printf("[\n")
}

#probe kernel.function("vfs_open") {
probe vfs.read {
   	list[pid(), ppid()] = gettimeofday_us()		
}

#probe kernel.function("vfs_open").return {
probe vfs.read.return {
	if([pid(),ppid()] in list)
		if(execname() != "stapio")
			// printf("%s, PID %d, PPID %d, %s,  Latency  %d sec\n", ctime(gettimeofday_s()) ,  pid(), ppid(), execname(), gettimeofday_us()-list[pid(),ppid()])
			printf("{\"time\": %d, \"pid\": %d, \"ppid\": %d, \"execname\": \"%s\", \"latency\": %d},\n", gettimeofday_s() ,  pid(), ppid(), execname(), gettimeofday_us()-list[pid(),ppid()])
}

probe end {
	printf("{}\n]\n");
}

